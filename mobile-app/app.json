{
  "expo": {
    "name": "Curry House Jar",
    "slug": "curry-house-jar",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.curryhousejar.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B35"
      },
      "package": "com.curryhousejar.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-asset"],
    "scheme": "curryhousejar"
  }
}
