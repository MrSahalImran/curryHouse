{
  "expo": {
    "name": "Curry House Jar",
    "slug": "curry-house-jar",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.curryhousejar.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B35"
      },
      "package": "com.curryhousejar.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-asset"],
    "scheme": "curryhousejar",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cae9cfe0-d5c3-428e-b39b-8b8bcc5bdd8c"
      }
    }
  }
}
